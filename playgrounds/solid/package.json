    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "eslint": "^9.15.0",
    "eslint-plugin-solid": "^0.14.4",
    "gh-pages": "^6.2.0",
    "globals": "^15.12.0",
    "solid-devtools": "^0.30.1",
    "typescript": "^5.6.3",
